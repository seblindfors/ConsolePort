	<Frame name="$parent_Alert" hidden="true">
		<Layers>
			<Layer>
				<Texture atlas="charcreatetest-tutorialbox-class">

				</Texture>
			</Layer>
		</Layers>
	</Frame>
	-->
	<Script file="PowerLevel.lua" />
</Ui>