<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Frame name="CPPowerLevel" parent="UIParent" enableMouse="false">
		<Frames>
			<Frame frameStrata="ARTWORK" name="PowerLevel_Widget">
				<Size x="145" y="24" />
				<Anchors>
					<Anchor relativeTo="UIParent" relativePoint="BOTTOM" point="CENTER" y="20" x="-5" />
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture atlas="jailerstower-wayfinder-rewardbackground-disable"
							name="$parent_Backdrop">
							<Size x="105" y="35" />
							<Anchors>
								<Anchor point="CENTER" x="2" y="0" />
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture atlas="bonusobjectives-bar-frame" name="$parent_Frame">
							<Size x="120" y="25" />
							<Anchors>
								<Anchor point="RIGHT" />
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture name="$parent_Icon">
							<Size x="25" y="25" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" />
							</Anchors>
						</Texture>
						<FontString text="" name="$parent_LevelText"
							inherits="GameFontHighlightSmallOutline">
							<Size x="130" y="12" />
							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent_Frame" x="4" y="-11" />
							</Anchors>
						</FontString>
					</Layer>
					<Layer level="LOW">
						<Texture atlas="ui-hud-experiencebar-fill-honor" name="$parent_Critical">
							<Size x="6" y="8" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" x="30" />
							</Anchors>
							<Animations>
								<AnimationGroup parentKey="Animation" setToFinalAlpha="true" looping="REPEAT">
									<Alpha duration="0.25" order="1" fromAlpha="0.30" toAlpha="1" />
									<Alpha duration="0.25" order="2" fromAlpha="1" toAlpha="0.30"
										startDelay="0.25" />
								</AnimationGroup>
							</Animations>
						</Texture>
						<Texture atlas="ui-hud-experiencebar-fill-honor" name="$parent_Low">
							<Size x="30" y="8" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" x="30" />
							</Anchors>
						</Texture>
						<Texture atlas="ui-hud-experiencebar-fill-reputation-faction-orange"
							name="$parent_Medium">
							<Size x="62" y="8" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" x="30" />
							</Anchors>
						</Texture>
						<Texture atlas="ui-hud-experiencebar-fill-reputation-faction-green"
							name="$parent_High">
							<Size x="93" y="8" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" x="30" />
							</Anchors>
						</Texture>
						<Texture atlas="ui-hud-experiencebar-fill-rested" name="$parent_Charging">
							<Size x="30" y="8" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" x="30" />
							</Anchors>
							<Animations>
								<AnimationGroup parentKey="Animation" looping="REPEAT">
									<Scale fromScaleY="1" toScaleY="1" fromScaleX="0" smoothing="NONE"
										toScaleX="1" duration="0.25" order="1" startDelay="0.25">
										<Origin point="LEFT" />
									</Scale>
									<Scale fromScaleY="1" toScaleY="1" fromScaleX="1" smoothing="NONE"
										toScaleX="2" duration="0.25" order="2" startDelay="0.5">
										<Origin point="LEFT" />
									</Scale>
									<Scale fromScaleY="1" toScaleY="1" fromScaleX="1" smoothing="NONE"
										toScaleX="1.55" duration="0.25" order="3" startDelay="0.75">
										<Origin point="LEFT" />
									</Scale>
									<Scale fromScaleY="1" toScaleY="1" fromScaleX="1" smoothing="NONE"
										toScaleX="1" duration="0.25" order="4" startDelay="1.0">
										<Origin point="LEFT" />
									</Scale>
								</AnimationGroup>
							</Animations>
						</Texture>
						<Texture atlas="ui-hud-experiencebar-background" name="$parent_Unknown">
							<Size x="93" y="8" />
							<Anchors>
								<Anchor point="LEFT" relativeKey="$parent_Frame" x="30" />
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
			<!--
			<Frame name="$parent_Alert" hidden="true">
				<Layers>
					<Layer>
						<Texture atlas="charcreatetest-tutorialbox-class">

						</Texture>
					</Layer>
				</Layers>
			</Frame>
			-->
		</Frames>
	</Frame>
	<Script file="PowerLevel.lua" />
</Ui>